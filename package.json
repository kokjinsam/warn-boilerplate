{
  "name": "apollo-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:server\" \"npm run start:client\"",
    "start:client": "npm run webpack:client",
    "start:server": "npm run node:server | npm run webpack:server",
    "node:server": "node node_modules/just-wait --pattern \"build/*.js\" && cross-env NODE_ENV=development nodemon ./build/server.js",
    "webpack:client": "cross-env NODE_ENV=development node ./tools/webpack/client/server.js",
    "webpack:server": "cross-env NODE_ENV=development webpack --watch --verbose --colors --display-error-details --config ./tools/webpack/server/dev.babel.js",
    "build": "npm run build:client | npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --colors --display-error-details --config ./tools/webpack/client/prod.babel.js",
    "build:server": "cross-env NODE_ENV=production webpack --colors --display-error-details --config ./tools/webpack/server/prod.babel.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "jss": "^5.1.0",
    "jss-camel-case": "^1.1.0",
    "jss-nested": "^1.0.3",
    "jss-vendor-prefixer": "^2.0.0",
    "lodash.pull": "^4.0.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-jss": "^2.1.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-require": "^2.1.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.3.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-jsx-control-statements": "^2.1.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.29.0",
    "flow-status-webpack-plugin": "^0.1.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "jsx-control-statements": "^3.1.2",
    "just-wait": "^1.0.5",
    "nodemon": "^1.9.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.3.0",
    "webpack-validator": "^2.2.3"
  }
}
